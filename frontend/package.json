{
  "name": "assistlink",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "start": "expo start",
    "start:go": "expo start --go",
    "start:offline": "expo start --offline",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "android:maps": "EXPO_PUBLIC_USE_REAL_MAPS=true expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:tailwind": "npx tailwindcss -i src/tailwind.css -o public/tailwind.css",
    "build:web": "npm run build:tailwind && expo export --platform web && npx workbox-cli generateSW workbox-config.js",
    "deploy:web": "npm run build:web && cp oauth-redirect.html dist/ && firebase deploy --only hosting",
    "build:android": "eas build --platform android --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:android:local": "npx expo prebuild --platform android --clean && cd android && ./gradlew assembleDebug && cd ..",
    "build:android:release": "npx expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease && cd .."
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.5.2",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@sentry/react-native": "^8.0.0",
    "@supabase/supabase-js": "^2.97.0",
    "expo": "~54.0.30",
    "expo-asset": "~12.0.12",
    "expo-auth-session": "^7.0.10",
    "expo-av": "~16.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-local-authentication": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "i18next": "^25.8.13",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "^1.20.1",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.2",
    "react-native-webrtc": "^124.0.7",
    "react-native-webview": "13.15.0",
    "react-router-dom": "^7.11.0"
  },
  "private": true,
  "devDependencies": {
    "@react-native-community/cli": "^20.1.1",
    "@react-native-community/cli-platform-android": "^20.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "~19.1.10",
    "autoprefixer": "^10.4.24",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2",
    "vitest": "^2.1.9",
    "workbox-cli": "^7.4.0"
  }
